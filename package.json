{
  "name": "telco-fusion",
  "version": "1.0.0",
  "description": "Telco Fusion APP ",
  "main": "index.ts",
  "scripts": {
    "start:prod": "cross-env APP_TYPE=PROD node --no-warnings --loader ts-node/esm index.ts",
    "start": "nodemon src/index.ts",
    "start:two": "node --no-warnings --loader ts-node/esm ./dist/index.js",
    "start:dev": "cross-env APP_TYPE=DEV node --trace-warnings --loader ts-node/esm index.ts",
    "build": "rimraf dist && tsc && npm run copy",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "copy":"mkdir dist/app/views/ && mkdir dist/public && mkdir dist/app/locales/ && cp -rf src/public/* dist/public/ && cp -rf src/app/views/* dist/app/views/ && cp -rf src/app/locales/* dist/app/locales/ && cp -rf src/serviceAccount.json dist/",
    "copy_NW": "copyfiles -u 1 src/app/views/**/* dist/ && copyfiles -u 1 src/public/**/* dist/ && copyfiles -u 1 src/app/locales/**/* dist/ && copyfiles -u 1 src/serviceAccount.json dist/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "url": "https://github.com/drycov/telco-fusion.git"
  },
  "author": "d.rykov",
  "license": "Apache-2.0",
