<head>
  <meta charset="utf-8" />
  <base href="./">
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta http-equiv="Cache-Control" content="no-store, max-age=0">
  <meta http-equiv="Pragma" content="no-cache">
  <meta http-equiv="Expires" content="0">
  <meta name="viewport"
    content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
  <meta name="theme-color" content="#ffffff" />
  <meta name="msapplication-TileColor" content="#ffffff" />
  <link rel="manifest" href="/public/manifest.json">
  <link rel="icon" type="image/x-icon" href="/favicon.ico" />

  <title><%= typeof title !== 'undefined' ? title : 'default' %></title>
  <% const gFonts = [
  'Roboto',
  'Oxygen',
  'Ubuntu',
  'Cantarell',
  'Fira Sans'
  ];
  %>
  <% gFonts.forEach(gFont => {
  %>
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css?family=<%= gFont %>">
  <% }); %>

  <% const cssFiles = [
  '/public/css/main.css',
  '/static/bootstrap/css/bootstrap.min.css',
  '/static/fortawesome/css/all.min.css',
  '/static/coreui/coreui/css/coreui.min.css',
  '/static/coreui/chartjs/css/coreui-chartjs.min.css',
  '/static/coreui/icon/css/all.min.css',
  '/static/tippy.js/tippy.css',
  '/static/select2/css/select2.min.css'
  ];
  %>

  <% cssFiles.forEach(cssFile => {
  %>
  <link rel="stylesheet" type="text/css" href="<%= cssFile %>" />
  <% }); %>

  <style>

  </style>
  <script
    src="https://www.google.com/recaptcha/enterprise.js?render=6LfnYbsoAAAAAL0cjT6_3RFSQvxPWABaFFGsUeBx"
    async defer></script>

</head>
