// Функция для форматирования чисел с разделителями тысяч
    var formatThousandsNoRounding = function(n) {
  return n.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
};
// Функция для анимации чисел
var animateNumbers = function() {
  $('.number').each(function() {
    var $this = $(this);
    var countTo = $this.attr('data-count');

    $({ countNum: $this.text()}).animate({
      countNum: countTo
    },
    {
      duration: 2000,
      easing: 'linear',
      step: function() {
        $this.text(formatThousandsNoRounding(Math.floor(this.countNum)));
      },
      complete: function() {
        $('#countUp').removeClass('loading');
        $('#pt3').fadeIn()
    $('#tme').fadeIn()
        
        $this.text(formatThousandsNoRounding(this.countNum));
      }
    });
  });
};

// Запуск анимации через setTimeout после задержки (например, 1 секунда)
setTimeout(function() {
    $('#pt3').hide();
    $('#tme').hide();
  animateNumbers();
}, 0); // 1000 миллисекунд = 1 секунда

